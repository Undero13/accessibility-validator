{
  "name": "wd",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "lint": "nodemon --exec \"npm run eslint\"",
    "eslint": "eslint --fix **/**.js",
    "eslint-nofix": "eslint **/**.js",
    "test": "jest",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Accessibility validator\"",    
    "package-linux": "electron-packager . --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds"
  },
  "author": "Patryk Loter",
  "license": "MIT",
  "devDependencies": {
    "electron-packager": "^14.1.1",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "jest": "^24.9.0",
    "nodemon": "^1.19.4",
    "prettier-eslint": "^9.0.0"
  },
  "dependencies": {
    "color": "^3.1.2",
    "css": "^2.2.4",
    "dom-parser": "^0.1.6",
    "dotenv": "^8.2.0",
    "electron": "^7.0.0",
    "electron-localizer": "^0.1.1",
    "nightmare": "^3.0.2"
  }
}
